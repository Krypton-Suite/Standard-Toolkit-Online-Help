{
  "metadata": [
    {
        "src": [
            {
                "files": [
                    "Krypton.Docking/Krypton.Docking 2022.csproj",
                    "Krypton.Navigator/Krypton.Navigator 2022.csproj",
                    "Krypton.Ribbon/Krypton.Ribbon 2022.csproj",
                    "Krypton.Toolkit/Krypton.Toolkit 2022.csproj",
                    "Krypton.Workspace/Krypton.Workspace 2022.csproj"
                ],
                "exclude": [
                    "**/obj/**",
                    "**/bin/**",
                    "..\\/**"
                ]
                ,"src": "../../../Source/Krypton Components/"
            }
        ],
        "dest": "api",
        "disableGitFeatures": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "articles/toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "..\\/**"
        ]
      }
    ],
    "resource": [
        {
            "files": [
                "articles/**.png"
            ],
            "exclude": [
                "obj/**",
                "..\\/**"
            ]
        },
        {
            "files": [
                "https://github.com/Krypton-Suite/Documentation/blob/main/Assets/NuGet%20Package%20Icons/Krypton%20Standard%20Toolkit%20Stable.png",
                "https://raw.githubusercontent.com/Krypton-Suite/Standard-Toolkit/master/Assets/ICO/Main%20Icon/48%20x%2048/Krypton%20Stable.ico"
            ]
        }
    ],
    "overwrite": [
        {
            "files": [
                "apidoc/**.md"
            ],
            "exclude": [
                "obj/**",
                "..\\/**"
            ]
        }
    ],
    "globalMetadata": {
        "_appTitle": "Krypton Standard Toolkit Documentation",
        "_appFooter": "<span>Krypton Component Suite 2024\nBSD 3-Clause License\nÂ© Component Factory Pty Ltd, 2006 - 2016, All rights reserved.</span> <span>Modifications by Peter Wagner (aka Wagnerp), Simon Coghlan (aka Smurf-IV), Giduac & Ahmed Abdelhameed et al. 2017 - 2025. All rights reserved. <a href=\"https://github.com/Krypton-Suite/Standard-Toolkit\">https://github.com/Krypton-Suite/Standard-Toolkit</a></span>",
        "_appLogoPath": "Logo.png",
        "_appFaviconPath": "Krypton.ico",
        "_disableContribution": true
    },

    "dest": "..\\Output",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "docfx-tmpl/src"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}