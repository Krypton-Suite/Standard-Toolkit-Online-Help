{
  "metadata": [
    {
        "src": [
            {
                "files": [
                    "Krypton.Docking/Krypton.Docking 2022.csproj",
                    "Krypton.Navigator/Krypton.Navigator 2022.csproj",
                    "Krypton.Ribbon/Krypton.Ribbon 2022.csproj",
                    "Krypton.Toolkit/Krypton.Toolkit 2022.csproj",
                    "Krypton.Workspace/Krypton.Workspace 2022.csproj"
                    // Do not add the TestApp
                ],
                "exclude": [
                    "**/obj/**",
                    "**/bin/**",
                    "..\\/**"
                ]
                ,"src": "./../../Source/Krypton Components/"
            }
        ],
        "dest": "api",
        "disableGitFeatures": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "articles/toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "..\\/**"
        ]
      }
    ],
    "resource": [
        {
            "files": [
                "articles/**.png"
            ],
            "exclude": [
                "obj/**",
                "..\\/**"
            ]
        },
        {
            "files": [
                "Logo.png",
                "Krypton.ico"
            ]
        }
    ],
    "overwrite": [
        {
            "files": [
                "apidoc/**.md"
            ],
            "exclude": [
                "obj/**",
                "..\\/**"
            ]
        }
    ],
    "globalMetadata": {
        "_appTitle": "Krypton Component Suite 2024\nBSD 3-Clause License\nï¿½ Component Factory Pty Ltd, 2006 - 2016, All rights reserved.",
        "_appFooter": "<span>Modifications by Peter Wagner (aka Wagnerp) & Simon Coghlan (aka Smurf-IV) et al. 2017 - 2024. All rights reserved. <a href=\"https://github.com/Krypton-Suite/Standard-Toolkit\">https://github.com/Krypton-Suite/Standard-Toolkit</a></span>",
        "_appLogoPath": "Logo.png",
        "_appFaviconPath": "Krypton.ico",
        "_disableContribution": true
    },

    "dest": "..\\Output",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "docfx-tmpl/src"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}